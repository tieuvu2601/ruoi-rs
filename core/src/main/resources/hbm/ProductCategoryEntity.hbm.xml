<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
	<class name="com.banvien.portal.vms.domain.ProductCategoryEntity" table="ProductCategory">

        <id name="productCategoryId" column="productCategoryId">
            <generator class="increment"/>
        </id>

		<property name="code" column="code" not-null="true" unique="true"/>

        <property name="name" column="name"/>
	
		<property name="displayOrder" column="displayOrder"/>

        <many-to-one name="parent" column="parentId" lazy="false"/>
	</class>
</hibernate-mapping>