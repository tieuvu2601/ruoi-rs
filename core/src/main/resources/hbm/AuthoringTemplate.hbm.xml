<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
	<class name="com.banvien.portal.vms.domain.AuthoringTemplate" table="AuthoringTemplate">
        <id name="authoringTemplateId" column="authoringTemplateId">
            <generator class="increment"/>
        </id>

		<property name="code" column="code"/>

        <property name="name" column="name"/>

        <property name="templateContent" column="templateContent"/>

        <property name="event" column="event"/>

		<property name="createdDate" column="createdDate"/>
	
		<property name="modifiedDate" column="modifiedDate"/>
	</class>
</hibernate-mapping>