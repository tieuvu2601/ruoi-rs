<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
	<class name="com.banvien.portal.vms.domain.AuthoringTemplateEntity" table="AuthoringTemplate">
        <id name="authoringTemplateId" column="AuthoringTemplateID">
            <generator class="increment"/>
        </id>

		<property name="code" column="Code"/>

        <property name="name" column="Name"/>

        <property name="templateContent" column="TemplateContent"/>

		<property name="createdDate" column="CreatedDate"/>
	
		<property name="modifiedDate" column="ModifiedDate"/>

	</class>
</hibernate-mapping>