<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
	<class name="com.banvien.portal.vms.domain.ProductTypeEntity" table="ProductType">
        <id name="productTypeId" column="productTypeId">
            <generator class="increment"/>
        </id>

		<property name="code" column="code" not-null="true" unique="true"/>

        <property name="name" column="name"/>
	
		<property name="displayOrder" column="displayOrder"/>

	</class>
</hibernate-mapping>