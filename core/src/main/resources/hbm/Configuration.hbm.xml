<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
	<class name="com.banvien.portal.vms.domain.ConfigurationEntity" table="configuration">
		<id name="configurationId" column="ConfigurationID">
            <generator class="increment"/>
		</id>
        <property name="hotLine" column="HotLine"/>

        <property name="aboutMe" column="AboutMe"/>

        <property name="customCSS" column="CustomCSS"/>
    </class>
</hibernate-mapping>