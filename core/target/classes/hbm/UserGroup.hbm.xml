<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
	<class name="com.banvien.portal.vms.domain.UserGroup" table="PortalUserGroup">

		<id name="userGroupID">
			<generator class="sequence">
                <param name="sequence">PORTALUSERGROUP_SEQ</param>
            </generator>
		</id>

		<property name="code" column="Code"/>

        <property name="name" column="Name"/>
	
		<property name="description" column="Description"/>
	
		<property name="createdDate" column="CreatedDate"/>
	
		<property name="modifiedDate" column="ModifiedDate"/>

	</class>
</hibernate-mapping>