<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
	<class name="com.banvien.portal.vms.domain.AuthoringTemplateUser" table="PortalAuthoringTemplateUser">

		<id name="authoringTemplateUserID">
			<generator class="sequence">
                <param name="sequence">PortalATU_SEQ</param>
            </generator>
		</id>

		<property name="authoringTemplateID" column="AuthoringTemplateID"/>

        <property name="userID" column="UserID"/>

	</class>
</hibernate-mapping>