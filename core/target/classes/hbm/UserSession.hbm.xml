<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
	<class name="com.banvien.portal.vms.domain.UserSession" table="UserSession">
        <id name="userSessionID">
            <generator class="sequence">
                <param name="sequence">PORTALUSERSESSION_SEQ</param>
            </generator>
        </id>
        <property name="numberOfVisitors" column="NumberOfVisitors" />
	</class>
</hibernate-mapping>