<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD 3.0//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd"> 

<hibernate-mapping>
	<class name="com.banvien.portal.vms.domain.AnnouncementCategory" table="PortalAnnouncementCategory">

		<id name="announcementCategoryID">
			<generator class="sequence">
                <param name="sequence">PORTALANNOUNCEMENT_SEQ</param>
            </generator>
		</id>

		<property name="url" column="URL"/>

        <property name="departmentID" column="DepartmentID"/>

        <property name="authoringTemplateID" column="AuthoringTemplateID"/>

        <property name="status" column="Status"/>

        <property name="fromDate" column="FromDate"/>

	</class>
</hibernate-mapping>